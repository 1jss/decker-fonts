{script:home.0}
on currfont do
 deck.fonts[first fontlist.rowvalue]
end

on view do
 fontlist.value:select name:key from deck.fonts
 f:currfont[]
 g:f[char.value-32]
 
 preview.font:f
 width    .value:f.size[0]
 height   .value:f.size[1]
 space    .value:f.space
 charwidth.value:g.size[0]
 charwidth.interval:1,width.value

 p:editor.pattern
 editor.lsize:f.size
 editor.scale:scale.value
 editor.clear[]
 editor.pattern:12
 editor.fill[]
 editor.pattern:1
 editor.paste[g]
 editor.pattern:p
end

on click do
 view[]
end

on change do
 view[]
end
{end}

{script:home.1}
on click pos do
 me.pattern:!me[pos]
end

on drag pos do
 send drag[pos]
 f:currfont[]
 g:me.copy[0,0 charwidth.value,height.value]
 f[char.value-32]:g
 view[]
end

on release pos do
 drag[pos]
end
{end}

{script:home.2}
on change val do
 f:currfont[]
 f.size:f.size[0],val
 view[]
end
{end}

{script:home.3}
on change val do
 f:currfont[]
 f.size:val,f.size[1]
 view[]
end
{end}

{script:home.4}
on change val do
 f:currfont[]
 f.space:val
end
{end}

{script:home.5}
on change val do
 f:currfont[]
 g:f[char.value-32]
 g.size:val,g.size[1]
 f[char.value-32]:g
 view[]
end
{end}

{script:home.6}
on click do
 f:currfont[]
 n:alert["New font name?" "string"]
 if n deck.add[f n] end
 view[]
end
{end}

{script:home.7}
on click do
 n:first fontlist.rowvalue
 if alert[("Are you sure you want to delete '%s'?" format n) "bool" "Delete"]
  deck.remove[currfont[]]
  view[]
 end
end
{end}

